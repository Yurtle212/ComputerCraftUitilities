-- get latest version
shell.run("delete startup")
shell.run("wget https://raw.githubusercontent.com/Yurtle212/ComputerCraftUitilities/main/minecraft/computers/hivemine_controller/startup")

shell.run("delete controller.lua")
shell.run("wget https://raw.githubusercontent.com/Yurtle212/ComputerCraftUitilities/main/minecraft/computers/hivemine_controller/controller.lua")

shell.run("delete json")
shell.run("wget https://pastebin.com/raw/4nRg9CHU json")

-- run script
shell.run("controller 5555")